global:
  scrape_interval:     15s 
  evaluation_interval: 15s 

alerting:
  alertmanagers:
  - static_configs:
    - targets: ['localhost:9093']

rule_files:
  # - "first_rules.yml"
  # - "second_rules.yml"


scrape_configs:
  - job_name: 'prometheus'
    scrape_interval: 5s
    scrape_timeout: 5s

    static_configs:
      - targets: ['localhost:9090']

  - job_name: monitor
    static_configs:
      - targets: ['localhost:9100']

  - job_name: 'front'
    scrape_interval: 5s
    static_configs:
      - targets: ['192.168.11.11:9100']
      
  - job_name: 'back1'
    scrape_interval: 5s
    static_configs: 
      - targets: ['192.168.11.12:9100']
      
  - job_name: 'back2'
    scrape_interval: 5s
    static_configs:
      - targets: ['192.168.11.13:9100']
      
  - job_name: 'db1'
    scrape_interval: 5s
    static_configs:
      - targets: ['192.168.11.14:9100']
      
  - job_name: 'db2'
    scrape_interval: 5s
    static_configs:
      - targets: ['192.168.11.15:9100']